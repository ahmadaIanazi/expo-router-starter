{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "@app/*": ["./app/*"],
      "@boards/*": ["./boards/*"],
      "@components/*": ["./components/*"],
      "@data/*": ["./data/*"],
      "@events/*": ["./events/*"],
      "@features/*": ["./features/*"],
      "@hooks/*": ["./hooks/*"],
      "@operations/*": ["./operations/*"],
      "@providers/*": ["./providers/*"],
      "store/*": ["./stores/*"],
      "@utils/*": ["./utils/*"],
      "@validations/*": ["./validations/*"],
      "@widgets/*": ["./widgets/*"],
      "@xSetup/*": ["./xSetup/*"],
      "@context/*": ["./xSetup/context/*"],
      "@constants/*": ["./xSetup/constants/*"],
      "ui": ["./xSetup/ui/index"],
      "api/*": ["./xSetup/api/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "xSetup/ui/components/index.jsx"
  ]
}